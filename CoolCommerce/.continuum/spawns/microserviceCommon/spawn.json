{
  "propertySchema":{
    "artifactId": {
      "type": "string",
      "description": "Maven Artifact Id"
    },
    "name": {
      "type": "string",
      "description": "Microservice Name",
      "default": "{{artifactId | camelCase | upperFirst}}"
    },
    "basePackage": {
      "type": "string",
      "description": "Base java package for application",
      "default": "{{groupId | append: '.' | append: artifactId | encodePackage}}"
    }
  }
}